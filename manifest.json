{
  "manifest_version": 2,
  "name": "Tiny Find Cache",
  "version": "1.0",
  "description": "Find cache URL",
  "icons" : {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "background" : {
    "persistent": false,
    "scripts": [ "background.js" ]
  },
  "permissions": [
    "<all_urls>",
    "contextMenus"
  ]
}
